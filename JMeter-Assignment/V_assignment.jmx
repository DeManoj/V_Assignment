            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonBuilder

def responseFile = new File(&quot;GET_Response1.json&quot;)
def json = new JsonSlurper().parseText(responseFile.text)

json.each { item -&gt;
    if (item.userId % 2 != 0 || item.id % 2 == 0) {
        item.completed = !item.completed
    }
}

new JsonBuilder(json).writeTo(new File(&quot;POST_Request.json&quot;))
